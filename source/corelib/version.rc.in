1 VERSIONINFO
FILEVERSION @PACKAGE_VERSION_MAJOR@,@PACKAGE_VERSION_MINOR@,@PACKAGE_VERSION_MICRO@,@PACKAGE_VERSION_PATCH@
PRODUCTVERSION @PACKAGE_VERSION_MAJOR@,@PACKAGE_VERSION_MINOR@,@PACKAGE_VERSION_MICRO@,@PACKAGE_VERSION_PATCH@
BEGIN
	BLOCK "StringFileInfo"
	BEGIN
		BLOCK "040904E4"
		BEGIN
			VALUE "CompanyName", "@PACKAGE_ORG@"
			VALUE "FileVersion", "@PACKAGE_VERSION@"
			VALUE "FileDescription", "@PACKAGE_DESCRIPTION@"
			VALUE "LegalCopyright", "@PACKAGE_COPYRIGHT@"
			VALUE "ProductName", "@PACKAGE_NAME@"
			VALUE "ProductVersion", "@PACKAGE_VERSION@"
		END
	END
	BLOCK "VarFileInfo"
	BEGIN
		VALUE "Translation", 0x409, 1252, 0x804, 1252
	END
END
